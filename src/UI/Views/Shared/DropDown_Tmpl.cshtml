@using Incoding.Web.Extensions
@model UI.Common.Models.DropdownModel

<div class="dropdown">
    <button class="btn btn-light" type="button" data-bs-toggle="dropdown" aria-expanded="false" title="@Model.Title">
        @Model.Text
    </button>
    <ul class="dropdown-menu">
        @foreach (var item in Model.Items)
        {
            @item.ToHtmlString()
        }
    </ul>
</div>