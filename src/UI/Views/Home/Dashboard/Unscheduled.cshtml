<div class="shadow rounded px-3 pt-3 d-flex flex-column gap-3 overflow-hidden" style="width: 45%; height: 704px;">
    <h3>@DataResources.UnscheduledClasses</h3>

    @(Html.When(JqueryBind.InitIncoding)
          .Ajax(Url.Dispatcher().Query<GetUnscheduledClassesQuery>().AsView("~/Views/Home/Dashboard/Tmpl/UnwantedCard_Tmpl.cshtml"))
          .OnSuccess(dsl => dsl.Self().Insert.Html())
          .AsHtmlAttributes(classes: "d-flex flex-column gap-2 overflow-hidden opacity-gradient")
          .ToDiv())
</div>
